<?xml version="1.0" encoding="utf-8"?>
<simulation>
  <type>wigner</type>
  <output_file>morse</output_file>
  <dataset>beta=10 props=16 4X4P</dataset>
  <parameters>
    <beta>10</beta>
    <num_atoms>1</num_atoms>
    <num_dimensions>1</num_dimensions>
    <num_propagators>16</num_propagators>
    <mass>1</mass>
    <potential name = "Morse">
        <rmin>0</rmin>
        <alpha>0.426401</alpha>
        <D>2.75</D>
    </potential>
    <estimator>PotentialEnergy, KineticEnergy</estimator>
  </parameters>
  <propagators>
    <propagator name="G6" number="15"/>
    <propagator name="WignerG2" number="1" dt="0.01" gamma="1" nx="4" np="4" alpha="1.52319"/>
  </propagators>
  <moves>
    <Translate>
      <length>1.75</length>
    </Translate>
    <BrownianBridge>
      <length>20</length>
      <attempts>5</attempts>
    </BrownianBridge>
  </moves>
  <monte_carlo>
    <num_MC>800000</num_MC>
    <num_blocks>10</num_blocks>
  </monte_carlo>
</simulation>
